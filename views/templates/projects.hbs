<h1>Project Gallery</h1>

{{#each project}}
<a href="/projects/{{this.id}}">
  <div class="projects">
    <p>link: {{this.link}}</p>
    <p>description: {{this.description}}</p>
  </div>
</a>
{{/each}}

<div>
  <input type=button class="hvr-wobble-horizontal" onClick="location.href='/projects/new'" value='New Project'>
</div>

<a href="/logout"><button>Logout</button></a>


